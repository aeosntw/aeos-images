        $(document).ready(function() {
            // Set the title dynamically
            $(document).prop('title', '404 - Page not found');
            
            // Remove all existing HTML in the body
            $('body').empty();

            // Create text elements
            var $heading = $('<h1>', { text: '404', class: 'big-text' });
            var $subheading = $('<p>', { text: 'The page you\'re looking for is nowhere to be found', class: 'subheading' });

            // Append text elements to the body
            $('body').append($heading, $subheading);

            // Create a new image element
            var $img = $('<img>', {
                src: 'https://media1.tenor.com/m/_BiwWBWhYucAAAAd/what-huh.gif',
                class: 'centered-image'
            });

            // Append the image to the body
            $('body').append($img);

            // Center the image on the screen
            $img.css({
                display: 'block',
                margin: '10vh auto 0', // Adjusted margin-top to position the image slightly lower
                maxWidth: '100%',
                height: 'auto'
            });

            // Style for big text
            $heading.css({
                marginTop:'50px',
                fontSize: '5em',
                fontWeight: 'bold',
                color: '#666', // Adjusted the color
