                <script>
(() => {
   
  const CLARITY_ID =  's5s3u5euwi';

 
  const AD_SLOT_SELECTOR = '.ad-slot';   // <div id="ad-top" class="ad-slot" data-ad-unit="/8095840/home_top" data-sizes='[[320,50],[728,90]]'></div>
  const AD_ROOT_MARGIN   = '600px 0px';   

  // ====== STUBS / HELPERS ====================================================
  window.googletag = window.googletag || { cmd: [] };
  window.clarity   = window.clarity   || function(){ (clarity.q = clarity.q || []).push(arguments); };

  const load = (src, {async=true,defer=true,attrs={},timeout=0}={}) => new Promise((res, rej) => {
    const s = document.createElement('script');
    s.src = src; s.async = async; s.defer = defer;
    Object.entries(attrs||{}).forEach(([k,v]) => s.setAttribute(k,v));
    s.onload = () => res();
    s.onerror = () => rej(new Error('Failed: '+src));
    document.head.appendChild(s);
    if (timeout) setTimeout(res, timeout);  
  });

  const idle = (fn, t=1200) =>
    ('requestIdleCallback' in window) ? requestIdleCallback(fn, { timeout: t }) : setTimeout(fn, t);

  const lowEnd = (navigator.deviceMemory && navigator.deviceMemory <= 2) || (navigator.hardwareConcurrency && navigator.hardwareConcurrency <= 4);

  const waitForConsent = () => new Promise(resolve => {
    if (!window.__tcfapi) return setTimeout(resolve, 1200);
    try {
      window.__tcfapi('addEventListener', 2, (tcData, ok) => {
        if (!ok) return;
        const done = tcData.eventStatus === 'useractioncomplete' ||
                     tcData.eventStatus === 'tcloaded' ||
                     tcData.gdprApplies === false;
        if (done) resolve();
      });
    } catch (_){ setTimeout(resolve, 1200); }
  });

  const waitForLCP = (max=1500) => new Promise(resolve => {
    let fired = false;
    try {
      const po = new PerformanceObserver(list => {
        if (list.getEntries().length) { fired = true; po.disconnect(); resolve(); }
      });
      po.observe({ type: 'largest-contentful-paint', buffered: true });
    } catch (_){}
    setTimeout(()=>{ if (!fired) resolve(); }, max);
  });

  // ====== STARTERS ============================================================
  function startClarity() {
    load('https://www.clarity.ms/tag/' + CLARITY_ID, { async:true, defer:true })
      .catch(()=>{});
  }
 
  let gptReady = false;
  function startAds() {
    if (gptReady) return; gptReady = true;
 
    load('https://securepubads.g.doubleclick.net/tag/js/gpt.js', { async:true, defer:true })
      .then(() => {
        googletag.cmd.push(() => {
          googletag.pubads().enableAsyncRendering();
          googletag.pubads().disableInitialLoad();  
          googletag.pubads().collapseEmptyDivs();
          // googletag.pubads().setCentering(true);  
          // googletag.pubads().setTargeting('page', 'home');
          googletag.enableServices();
        });
        initSlotLazy();       
      });

    
    idle(() => load('https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js').catch(()=>{}), lowEnd ? 2000 : 800);
    idle(() => load('https://fms.360yield.com/ow/bundles/live/pubgalaxy/publishers/1gamesio.min.js').catch(()=>{}), lowEnd ? 2600 : 1200);
    idle(() => load('https://btloader.com/tag?o=5184339635601408&upapi=true').catch(()=>{}), lowEnd ? 3200 : 1600);
  }

  // ====== SLOT LAZY LOADING ===================================================
  const slotMap = new Map(); // elementId -> GPT slot
  function parseSizes(json) {
    try { return JSON.parse(json); } catch(_){ return null; }
  }

  function ensureDefineSlot(el) {
    const unit  = el.getAttribute('data-ad-unit');       
    const sizes = parseSizes(el.getAttribute('data-sizes'));  
    if (!unit || !sizes) return null;

    const id = el.id || ('ad_' + Math.random().toString(36).slice(2));
    if (!el.id) el.id = id;

    if (slotMap.has(id)) return slotMap.get(id);

    let slot = null;
    googletag.cmd.push(() => {
      slot = googletag.defineSlot(unit, sizes, id).addService(googletag.pubads());
      slotMap.set(id, slot);
    });
    return slot;
  }

  function displayAndRefresh(el) {
    const id = el.id;
    googletag.cmd.push(() => {
       
      if (!el.dataset.displayed) {
        googletag.display(id);
        el.dataset.displayed = '1';
      }
      const slot = slotMap.get(id);
      if (slot) googletag.pubads().refresh([slot]);
    });
  }

  function initSlotLazy() {
    const els = document.querySelectorAll(AD_SLOT_SELECTOR);
    if (!els.length) return;
 
    els.forEach(ensureDefineSlot);
 
    const atf = Array.from(els).filter(el => el.dataset.atf === '1');
    atf.forEach(el => displayAndRefresh(el));
 
    if (!('IntersectionObserver' in window)) {
      Array.from(els).forEach(el => displayAndRefresh(el));
      return;
    }
    const io = new IntersectionObserver((entries) => {
      entries.forEach(e => {
        if (!e.isIntersecting) return;
        displayAndRefresh(e.target);
        io.unobserve(e.target);
      });
    }, { rootMargin: AD_ROOT_MARGIN, threshold: 0 });

    els.forEach(el => { if (!el.dataset.atf) io.observe(el); });
  }

  // ====== ORCHESTRATION =======================================================
  document.addEventListener('DOMContentLoaded', () => {
    Promise.all([waitForConsent(), waitForLCP(lowEnd ? 1800 : 1200)]).then(() => { 
      idle(startAds, lowEnd ? 400 : 100);
      idle(startClarity, lowEnd ? 1200 : 600);
    });
  });
 
  window.addEventListener('load', () => {
    ['https://securepubads.g.doubleclick.net',
     'https://pagead2.googlesyndication.com',
     'https://fms.360yield.com',
     'https://btloader.com',
     'https://www.clarity.ms'].forEach(h => {
      const l = document.createElement('link'); l.rel='dns-prefetch'; l.href=h; document.head.appendChild(l);
    });
  });
})();
</script>
